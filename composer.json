{
    "name": "etna/rabbitmq-service-provider",
    "description": "RabbitMQ Service Provider",
    "keywords": ["rabbitmq", "silex"],
    "license": "proprietary",
    "authors": [
        {
            "name": "ETNA",
            "email": "dev@etna-alternance.net",
            "homepage": "http://etna-alternance.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "silex/silex": "~2.0@dev",
        "videlalvaro/php-amqplib": "2.x",
        "oldsound/rabbitmq-bundle": "~1.8",
        "etna/silex-console": "2.x"
    },
    "require-dev": {
        "behat/behat": "2.x",
        "etna/composer-behat-utils": "0.x"
